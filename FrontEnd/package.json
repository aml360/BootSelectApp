{
	"name": "front-end",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:dev": "rimraf dist && ng serve",
		"build": "ng build --configuration production",
		"build:prod": "ng build --configuration production && npm run removecss",
		"removecss": "cd dist/FrontEnd && purgecss --css ./styles.css --content *.html *.js --output ./styles.css",
		"copy2rust": "cd dist/FrontEnd && cp -r * ../../../backend_rs/front_dist",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"compodoc": "compodoc -p tsconfig.json -s",
		"commit": "cz"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^12.2.3",
		"@angular/common": "^12.2.3",
		"@angular/compiler": "^12.2.3",
		"@angular/core": "^12.2.3",
		"@angular/forms": "^12.2.3",
		"@angular/material": "^12.2.3",
		"@angular/platform-browser": "^12.2.3",
		"@angular/platform-browser-dynamic": "^12.2.3",
		"@angular/router": "^12.2.3",
		"@auth0/angular-jwt": "^5.0.2",
		"@capacitor/android": "^3.2.0",
		"@capacitor/cli": "^3.2.0",
		"@capacitor/core": "^3.2.0",
		"aos": "^2.3.4",
		"bootstrap": "^5.1.0",
		"date-fns": "^2.23.0",
		"js-cookie": "^2.2.1",
		"lodash": "^4.17.21",
		"ngx-material-timepicker": "^5.5.3",
		"rxjs": "^6.6.7",
		"tslib": "^2.3.1",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^12.2.3",
		"@angular/cdk": "^12.2.3",
		"@angular/cli": "^12.2.3",
		"@angular/compiler-cli": "^12.2.3",
		"@compodoc/compodoc": "^1.1.14",
		"@types/aos": "^3.0.4",
		"@types/jasmine": "^3.8.2",
		"@types/js-cookie": "^2.2.7",
		"@types/lodash": "^4.14.173",
		"@types/node": "^16.7.6",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"jasmine-core": "~3.9.0",
		"jasmine-spec-reporter": "~7.0.0",
		"karma": "^6.3.4",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "^2.0.3",
		"karma-jasmine": "^4.0.1",
		"karma-jasmine-html-reporter": "^1.7.0",
		"protractor": "~7.0.0",
		"purgecss": "^4.0.3",
		"schematics-scss-migrate": "^1.3.13",
		"ts-node": "~10.2.1",
		"typescript": "~4.3.5"
	},
	"config": {
		"commitizen": {
			"path": "./FrontEnd/node_modules/cz-conventional-changelog",
			"maxHeaderWidth": 95
		}
	}
}
