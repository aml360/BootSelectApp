{
  "type": "postgres",
  "host": "yourDbHostName",
  "port": 5432,
  "username": "postgres",
  "password": "yourDbPass",
  "database": "postgres",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/entity/**/*.entity{.ts,.js}"
  ],
  "migrations": [
    "src/database/migrations/*{.ts,.js}"
  ],
  "subscribers": [
    "src/subscriber/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/database/migrations",
    "subscribersDir": "src/subscriber"
  }
}
